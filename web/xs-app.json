{
  "welcomeFile": "/web/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
	{
	    "source": "(.*)(.xsjs)",
	    "destination": "app-xsjs_api",
	    "csrfProtection": true,
	    "authenticationType": "xsuaa"
	}, {
	    "source": "(.*)(.xsodata)",
	    "destination": "app-xsjs_api",
	    "csrfProtection": true,
	    "authenticationType": "xsuaa"
	}, {
      "source": "^/web/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }	
  ]
}