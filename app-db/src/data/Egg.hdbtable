COLUMN TABLE "app_db::Space.Egg"(
	"ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000 INCREMENT BY 1 MAXVALUE 9999) NOT NULL COMMENT 'Egg ID',
	"SIZE" DECIMAL(15,2) CS_FIXED COMMENT 'Size',
	PRIMARY KEY ("ID"))
	COMMENT 'Egg'
	UNLOAD PRIORITY 5 AUTO MERGE