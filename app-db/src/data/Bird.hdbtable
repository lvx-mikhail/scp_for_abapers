COLUMN TABLE "app_db::Space.Bird"(
	"ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000 INCREMENT BY 1 MAXVALUE 9999) NOT NULL COMMENT 'Bird ID',
	"NAME" NVARCHAR(10) COMMENT 'Name',
	"STATUS" NVARCHAR(1) COMMENT 'Status',
	"EGG.ID" INTEGER CS_INT COMMENT 'Egg',
	PRIMARY KEY ("ID"))
	WITH ASSOCIATIONS( JOIN "app_db::Space.Egg" AS "EGG" ON "EGG"."ID" = "EGG.ID" ) 
	COMMENT 'Bird'
	UNLOAD PRIORITY 5 AUTO MERGE